{
  "version": "1.0",
  "defaultProvider": "cdnjs",
  "libraries": [
    {
      "library": "jquery@3.6.0",
      "destination": "wwwroot/lib/jquery/"
    },
    {
      "provider": "jsdelivr",
      "library": "sweetalert2@11.4.24",
      "destination": "wwwroot/lib/sweetalert2/",
      "files": [
        "dist/sweetalert2.all.js",
        "dist/sweetalert2.all.min.js",
        "dist/sweetalert2.css",
        "dist/sweetalert2.js",
        "dist/sweetalert2.min.css",
        "dist/sweetalert2.min.js",
        "sweetalert2.d.ts"
      ]
    },
    {
      "provider": "jsdelivr",
      "library": "lightslider@1.1.6",
      "destination": "wwwroot/lib/lightslider/",
      "files": [
        "dist/js/lightslider.min.js",
        "dist/css/lightslider.css",
        "dist/css/lightslider.min.css",
        "dist/img/controls.png",
        "dist/js/lightslider.js"
      ]
    },
    {
      "library": "jqueryui@1.13.2",
      "destination": "wwwroot/lib/jqueryui/"
    }
  ]
}